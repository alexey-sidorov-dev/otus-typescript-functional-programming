{
  "name": "otus-typescript-functional-programming",
  "version": "1.0.0",
  "description": "OTUS TypeScript Functional Programming",
  "keywords": [
    "otus",
    "typescript",
    "functional programming"
  ],
  "homepage": "https://github.com/alexey-sidorov-dev/otus-typescript-functional-programming#readme",
  "bugs": {
    "url": "https://github.com/alexey-sidorov-dev/otus-typescript-functional-programming/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexey-sidorov-dev/otus-typescript-functional-programming.git"
  },
  "license": "ISC",
  "author": "Alexey Sidorov",
  "main": "./src/index.ts",
  "scripts": {
    "lint": "eslint . --config eslint.config.js --ext .js,.ts && tsc --project tsconfig.json --noEmit",
    "lint:all": "eslint . --config eslint.config.js --ext .js,.ts && tsc --project tsconfig.json --noEmit && prettier . --check",
    "lint:all:fix": "eslint . --config eslint.config.js --ext .js,.ts --fix && prettier . --write",
    "lint:fix": "eslint . --config eslint.config.js --ext .js,.ts --fix",
    "prepare": "husky install",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.5.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-jest": "^28.1.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-tsc": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "prettier-plugin-packagejson": "^2.2.18",
    "prettier-plugin-sh": "^0.11.0",
    "typescript": "^4.6.4"
  }
}
