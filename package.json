{
  "name": "otus-typescript-functional-programming",
  "version": "1.0.0",
  "description": "OTUS TypeScript Functional Programming",
  "keywords": [
    "otus",
    "typescript",
    "functional programming"
  ],
  "homepage": "https://github.com/alexey-sidorov-dev/otus-typescript-functional-programming#readme",
  "bugs": {
    "url": "https://github.com/alexey-sidorov-dev/otus-typescript-functional-programming/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexey-sidorov-dev/otus-typescript-functional-programming.git"
  },
  "license": "ISC",
  "author": "Alexey Sidorov",
  "main": "./src/index.ts",
  "scripts": {
    "lint": "eslint . --ext .js,.ts && tsc --project tsconfig.json --noEmit",
    "lint:all": "eslint . --ext .js,.ts && tsc --project tsconfig.json --noEmit && prettier . --check",
    "lint:all:fix": "eslint . --ext .js,.ts --fix && tsc --project tsconfig.json --noEmit && prettier . --write",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "prepare": "husky install",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "test": "jest",
    "test:coverage": "jest --coverage --silent"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "babel-jest": "^29.0.3",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "prettier-plugin-sh": "^0.12.8",
    "typescript": "^4.8.3"
  }
}
